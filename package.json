{
  "name": "typeorm-project",
  "version": "1.0.0",
  "description": "Local clone of TypeORM for migration-bounty work",
  "main": "build/compiled/index.js",
  "types": "build/compiled/index.d.ts",
  "files": [
    "build/compiled"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && mocha -r ts-node/register \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeorm/typeorm.git"
  },
  "keywords": [
    "typeorm",
    "migration",
    "postgres",
    "typescript"
  ],
  "author": "TheodorNEngoy",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "typescript": "^4.8.4",
    "ts-node": "^10.9.1",
    "mocha": "^10.2.0",
    "chai": "^4.3.7",
    "husky": "^8.0.0",
    "eslint": "^8.38.0",
    "cross-env": "^7.0.3"
  }
}
